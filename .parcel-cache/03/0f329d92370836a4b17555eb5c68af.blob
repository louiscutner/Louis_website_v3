const rightClick = function (list) {
  for (let x = 0; x < list.length; x++) {
    if (list[x].classList.contains("hidden")) {} else {
      if (x !== list.length - 1) {
        list[x].classList.add("hidden");
        list[x + 1].classList.remove("hidden");
      }
      break;
    }
  }
};
const leftClick = function (list) {
  for (let x = 0; x < list.length; x++) {
    if (list[x].classList.contains("hidden")) {} else {
      if (x !== 0) {
        list[x].classList.add("hidden");
        list[x - 1].classList.remove("hidden");
      }
      break;
    }
  }
};
